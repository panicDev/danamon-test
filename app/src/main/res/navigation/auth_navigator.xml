<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/auth_navigator"
    app:startDestination="@layout/fragment_login">

    <fragment
        android:id="@+id/fragment_login"
        android:name="com.danamon.test.feature.auth.login.ui.LoginFragment"
        tools:layout="@layout/fragment_login">

        <action
            android:id="@+id/to_register"
            app:destination="@+id/fragment_register"/>

    </fragment>

    <fragment
        android:id="@+id/fragment_register"
        android:name="com.danamon.test.feature.auth.register.ui.RegisterFragment"
        tools:layout="@layout/fragment_register"/>

</navigation>